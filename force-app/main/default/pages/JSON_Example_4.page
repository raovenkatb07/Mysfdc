<apex:page controller="JSON_Example_4">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockButtons >
            <apex:commandButton value="Submit" action="{!create}" />
            </apex:pageBlockButtons>
            <apex:pageBlockSection columns="1">
                <apex:pageBlockSectionItem >
                    <apex:outputText value="name"></apex:outputText>
                    <apex:inputText value="{!name}" />
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem >
                    <apex:outputText value="phone"></apex:outputText>
                    <apex:inputText value="{!phone}" />
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem >
                    <apex:outputText value="Annulrevenue"></apex:outputText>
                    <apex:inputText value="{!anul}" />
                </apex:pageBlockSectionItem>
            </apex:pageBlockSection>
            <apex:outputText value="{!result}"></apex:outputText>
        </apex:pageBlock>
    </apex:form>
</apex:page>